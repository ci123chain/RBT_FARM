{
  "name": "deor-farm",
  "version": "0.1.0",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "scripts/test.sh",
    "script:deploy": "truffle compile && truffle exec ./scripts/deploy.js --network development",
    "script:deploy-lock": "truffle compile && truffle exec ./scripts/deploy.js --network development --unlockAddress 0x36da2aa0484392a2632529fDEbcf08c56A246A62 --rbt 0x2b54D37C0aBa30fd5d948297CeFbc6a0ea527240",

    "script:deploy-ci123": "truffle compile && truffle exec ./scripts/deploy.js --network ci123",
    "script:deploy-ci123-lock": "truffle compile && truffle exec ./scripts/deploy.js --network ci123 --unlockAddress 0xC7b7222270a66c1BE931199a63dca6A01a478fDD --rbt 0xfeE69cF0b0bA1d3B4Fc6c3F1Da196BC20460D212",

    "script:deploy-okex": "truffle compile && truffle exec ./scripts/deploy.js --network okextest",

    "script:deploy-rbt-okex": "truffle compile && truffle exec ./scripts/okex/1_rbt.js --network okextest",
    "script:deploy-nft1155-okex": "truffle compile && truffle exec ./scripts/okex/2_nft1155.js --network okextest",
    "script:deploy-nftfarm-okex": "truffle compile && truffle exec ./scripts/okex/3_nftfarm.js --network okextest",
    "script:deploy-market-okex": "truffle compile && truffle exec ./scripts/okex/4_market.js --network okextest",
    "script:deploy-single-okex": "truffle compile && truffle exec ./scripts/okex/5_singlefarm.js --network okextest",
    "script:deploy-lockfarm-okex": "truffle compile && truffle exec ./scripts/okex/6_lockedfarm.js --network okextest",

    "script:deploy-lp-okex": "truffle compile && truffle exec ./scripts/okex/7_lpfarm.js --network okextest",


    "dev": "truffle migrate --network kovan --reset",
    "devverify": "truffle run verify Farm --network kovan",
    "migrate": "rm -rf build && truffle migrate --network mainnet --reset",
    "verify": "truffle run verify Farm --network mainnet"
  },
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "mocha": "^8.2.1",
    "mockjs": "^1.1.0"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.9",
    "@truffle/hdwallet-provider": "^1.2.6",
    "dotenv": "^8.2.0"
  }
}

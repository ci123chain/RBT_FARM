{
  "name": "deor-farm",
  "version": "0.1.0",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "scripts/test.sh",
    "script:deploy-nolock": "truffle compile && truffle exec ./scripts/deploy.js --network development",
    "script:deploy-lock": "truffle compile && truffle exec ./scripts/deploy.js --network development --unlockAddress 0x37B94b2c5939b35bBF39e0C3507Bd32198bE05b7 --rbt 0x923C2593f2A404546D093e7f70E1Ac17dFa7990b",

    "script:deploy-okex": "truffle compile && truffle exec ./scripts/deploy.js --network okextest",

    "dev": "truffle migrate --network kovan --reset",
    "devverify": "truffle run verify Farm --network kovan",
    "migrate": "rm -rf build && truffle migrate --network mainnet --reset",
    "verify": "truffle run verify Farm --network mainnet"
  },
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "mocha": "^8.2.1",
    "mockjs": "^1.1.0"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.9",
    "@truffle/hdwallet-provider": "^1.2.6",
    "dotenv": "^8.2.0",
    "ganache-cli": "^6.12.1",
    "truffle": "^5.1.52",
    "truffle-plugin-verify": "^0.5.0"
  }
}
